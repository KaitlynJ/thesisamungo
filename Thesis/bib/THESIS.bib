	keywords = {Health behavior, Health care intervention, Health care records, Survey design, Utilization, Validity of results},
	pages = {136--141},
	file = {ScienceDirect Snapshot:/Users/Kjackson/Library/Application Support/Zotero/Profiles/k98xgx8p.default/zotero/storage/IRUZS4H3/S0895435600002614.html:text/html}
}

@article{phillips_primary_2010,
