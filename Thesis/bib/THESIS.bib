@article{shaw_decision-making_2013,
	title = {Decision-{Making} {Processes} of {Patients} {Who} {Use} the {Emergency} {Department} for {Primary} {Care} {Needs}},
	volume = {24},
	issn = {1548-6869},
	url = {https://muse.jhu.edu/journals/journal_of_health_care_for_the_poor_and_underserved/v024/24.3.shaw.html},
	doi = {10.1353/hpu.2013.0140},
	abstract = {Emergency department (ED) use for non-urgent needs is widely viewed as a contributor to various health care system flaws and inefficiencies. There are few qualitative studies designed to explore the complexity of patients’ decision-making process to use the ED vs. primary care alternatives. In this study, semi-structured interviews were conducted with 30 patients who were discharged from the low acuity area of a university hospital ED. A grounded theory approach including cycles of immersion/crystallization was used to identify themes and reportable interpretations. Patients reported multiple decision-making considerations that hinged on whether or not they knew about primary care options. A model is developed depicting the complexity and variation in patients’ decision-making to use the ED. Optimizing health system navigation and use requires improving objective factors such as access and costs as well as subjective perceptions of patients’ health care, which are also a prominent part of their decision-making process.},
	number = {3},
	urldate = {2015-09-21},
	journal = {Journal of Health Care for the Poor and Underserved},
	author = {Shaw, Eric K. and Howard, Jenna and Clark, Elizabeth C. and Etz, Rebecca S. and Arya, Rajiv and Tallia, Alfred F.},
	year = {2013},
	note = {{\textless}p{\textgreater}Volume 24, Number 3, August 2013{\textless}/p{\textgreater}},
	pages = {1288--1305},
	file = {Project MUSE Full Text PDF:/Users/Kjackson/Library/Application Support/Zotero/Profiles/k98xgx8p.default/zotero/storage/BREKR2J8/Shaw et al. - 2013 - Decision-Making Processes of Patients Who Use the .pdf:application/pdf;Project MUSE Snapshot:/Users/Kjackson/Library/Application Support/Zotero/Profiles/k98xgx8p.default/zotero/storage/4U88C6V7/24.3.shaw.html:text/html}
